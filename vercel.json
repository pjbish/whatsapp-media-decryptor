{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "client/dist"
      }
  "buildCommand": "npm run build",
  "outputDirectory": "client/dist",
  "functions": {
    "api/decrypt.js": {
      "runtime": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/client/dist/index.html"
    }
  ]
  }
}
